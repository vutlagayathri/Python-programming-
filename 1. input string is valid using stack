def is_valid(s):
    stack = []
    mapping = {')': '(', '}': '{', ']': '['} 
    for char in s:
        if char in {'(', '{', '['}:
            stack.append(char)
        elif char in {')', '}', ']'}:
            if not stack:
                return False
            elif stack[-1] != mapping[char]:
                return False
            else:
                stack.pop() 
    return len(stack) == 0 
s = input("Enter the string: ")
if is_valid(s):
    print("The string is valid.")
else:
    print("The string is not valid.")




